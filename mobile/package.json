{
  "name": "music",
  "private": true,
  "main": "index.ts",
  "version": "2.6.0-rc.1",
  "description": "A Nothing inspired music player.",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/MissingCore/Music"
  },
  "author": "cyanChill",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:prod": "expo run:android --variant release",
    "atlas": "npx expo export --platform android && npx expo-atlas",
    "typecheck": "tsc --noEmit",
    "lint": "expo lint",
    "circular-dep": "npx dpdm ./src/* --circular --exit-code circular:1 --warning=false",
    "db:generate": "drizzle-kit generate",
    "gen:splashscreen": "npx react-native-bootsplash generate --platforms android --background #F2F2F2 ./assets/splash-icon.png",
    "sync:licenses": "license-checker-rseidelsohn --production --depth=0 --excludePrivatePackages --clarificationsFile ./scripts/licenses/licenseClarification.json --customPath ./scripts/licenses/customLicenseFormat.json --json | node ./scripts/licenses/updateLicenses.mjs",
    "release": "git pull && python ./scripts/release/release_bump.py"
  },
  "dependencies": {
    "@backpackapp-io/react-native-toast": "^0.13.0",
    "@boterop/react-native-background-timer": "^2.6.0",
    "@lodev09/react-native-true-sheet": "^2.0.6",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@miblanchard/react-native-slider": "github:MissingCore/react-native-slider#4fa4f2097bdabaf4bc3866202debc84482020d5f",
    "@missingcore/react-native-actual-path": "^0.1.0",
    "@missingcore/react-native-metadata-retriever": "^2.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-native-assets/slider": "^11.0.11",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/material-top-tabs": "^7.3.8",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "@sentry/react-native": "~7.2.0",
    "@shopify/flash-list": "^1.8.3",
    "@tanstack/react-query": "^5.90.2",
    "@weights-ai/react-native-track-player": "github:MissingCore/react-native-track-player#a8c5601b3675d83cdb99ee6a3a9c04c7db8df7fc",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "expo": "~53.0.23",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-drizzle-studio-plugin": "^0.2.0",
    "expo-file-system": "~18.1.11",
    "expo-image": "~2.4.0",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-localization": "~16.1.6",
    "expo-media-library": "~17.1.7",
    "expo-sqlite": "~15.2.14",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.5.3",
    "intl-pluralrules": "^2.0.1",
    "jotai": "^2.15.0",
    "nativewind": "4.1.23",
    "react": "19.0.0",
    "react-i18next": "^16.0.0",
    "react-native": "^0.79.6",
    "react-native-android-widget": "^0.17.2",
    "react-native-bootsplash": "^6.3.11",
    "react-native-draglist": "github:MissingCore/react-native-draglist#a3847ce4f9265853a12b865ba30902e907d43c20",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-markdown-display": "^7.0.2",
    "react-native-pager-view": "^6.9.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.19.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "tailwind-merge": "^2.6.0",
    "zod": "~4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@types/react": "~19.0.14",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "expo-atlas": "^0.4.3",
    "license-checker-rseidelsohn": "^4.4.2",
    "neatjson": "^0.10.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-splash-screen"
      ]
    }
  },
  "pnpm": {
    "overrides": {
      "markdown-it": "14.1.0"
    },
    "patchedDependencies": {
      "react-native-gesture-handler": "patches/react-native-gesture-handler.patch",
      "@react-native-assets/slider": "patches/@react-native-assets__slider.patch",
      "@backpackapp-io/react-native-toast": "patches/@backpackapp-io__react-native-toast.patch",
      "expo-file-system": "patches/expo-file-system.patch"
    }
  }
}

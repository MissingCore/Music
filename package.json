    "expo-font": "~12.0.7",
    "expo-image": "^1.12.12",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.0.6",
    "expo-linear-gradient": "~13.0.2",
    "expo-media-library": "~16.0.4",
    "expo-router": "~3.5.17",
    "expo-sharing": "~12.0.1",
    "expo-sqlite": "^14.0.4",
    "expo-web-browser": "~13.0.3",
    "jotai": "^2.8.4",
    "nativewind": "^4.0.36",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-bootsplash": "6.0.0-beta.9",
    "react-native-gesture-handler": "~2.16.2",

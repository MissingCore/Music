    "jest-expo": "~51.0.3",
    "license-checker-rseidelsohn": "^4.3.0",
    "neatjson": "^0.10.6",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "resolutions": {
    "markdown-it": "14.1.0"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-splash-screen"
      ]
    }
  }
